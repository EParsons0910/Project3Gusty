<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="150" xmlns:fx="http://javafx.com/fxml/1" fx:controller="basicStuff.SignInFXMLController">
    <children>
        <GridPane layoutX="75" layoutY="50" hgap="10" vgap="10" prefWidth="200">
            <TextField GridPane.columnIndex="0" GridPane.rowIndex="0" prefWidth="100" fx:id="username" />
            <TextField GridPane.columnIndex="0" GridPane.rowIndex="1" prefWidth="100" fx:id="password" />
            <Button GridPane.columnIndex="0" GridPane.rowIndex="2" text="Sign in" prefWidth="100" onAction="#signIn" fx:id="signInButton" />
        </GridPane>
    </children>
</AnchorPane>
